{
  "manifest_version": 2,
  "name": "StenoKeyboard - Dictionary plugin",
  "short_name": "StenoKeyboard-Dictionary",
  "description": "Use your personal steno dictionaries (in Plover format) with the StenoKeyboard chrome extension",
  "author": "Brent Nesbitt",
  "version": "0.0.4",
  "version_name": "v0.0 pre-release-4",
  "minimum_chrome_version": "38",

  "icons": {
    "128": "assets/icon_128.png"
  },
  
  "app": {
    "background": {
      "scripts": ["js/background.js"]
    }
  },


  "permissions": [
    "storage",
    {"fileSystem": ["retainEntries"]}
  ]

}
